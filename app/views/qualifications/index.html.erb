<p id="notice"><%= notice %></p>

<h1>Qualifications</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>School</th>
      <th>From</th>
      <th>To</th>
      <th>Grade</th>
      <th>Active</th>
      <th>Person</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualifications.each do |qualification| %>
      <tr>
        <td><%= qualification.name %></td>
        <td><%= qualification.school %></td>
        <td><%= qualification.from %></td>
        <td><%= qualification.to %></td>
        <td><%= qualification.grade %></td>
        <td><%= qualification.active %></td>
        <td><%= qualification.person_id %></td>
        <td><%= qualification.user_id %></td>
        <td><%= link_to 'Show', qualification %></td>
        <td><%= link_to 'Edit', edit_qualification_path(qualification) %></td>
        <td><%= link_to 'Destroy', qualification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qualification', new_qualification_path %>
