<p id="notice"><%= notice %></p>

<h1>Withdrawals</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Amount</th>
      <th>Details</th>
      <th>User</th>
      <th>Saving</th>
      <th>Institution</th>
      <th>Month</th>
      <th>Year</th>
      <th>Person</th>
      <th>Financialinstitution</th>
      <th>Origination</th>
      <th>Rationale</th>
      <th>Day</th>
      <th>Plannedactivity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @withdrawals.each do |withdrawal| %>
      <tr>
        <td><%= withdrawal.owner %></td>
        <td><%= withdrawal.amount %></td>
        <td><%= withdrawal.details %></td>
        <td><%= withdrawal.user_id %></td>
        <td><%= withdrawal.saving_id %></td>
        <td><%= withdrawal.institution %></td>
        <td><%= withdrawal.month %></td>
        <td><%= withdrawal.year %></td>
        <td><%= withdrawal.person_id %></td>
        <td><%= withdrawal.financialinstitution_id %></td>
        <td><%= withdrawal.origination %></td>
        <td><%= withdrawal.rationale %></td>
        <td><%= withdrawal.day %></td>
        <td><%= withdrawal.plannedactivity_id %></td>
        <td><%= link_to 'Show', withdrawal %></td>
        <td><%= link_to 'Edit', edit_withdrawal_path(withdrawal) %></td>
        <td><%= link_to 'Destroy', withdrawal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Withdrawal', new_withdrawal_path %>
