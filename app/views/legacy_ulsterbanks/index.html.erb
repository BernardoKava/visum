<p id="notice"><%= notice %></p>

<h1>Legacy Ulsterbanks</h1>

<table>
  <thead>
    <tr>
      <th>Trans date</th>
      <th>Trans type</th>
      <th>Description</th>
      <th>Trans value</th>
      <th>Account name</th>
      <th>Account number</th>
      <th>Account name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @legacy_ulsterbanks.each do |legacy_ulsterbank| %>
      <tr>
        <td><%= legacy_ulsterbank.trans_date %></td>
        <td><%= legacy_ulsterbank.trans_type %></td>
        <td><%= legacy_ulsterbank.description %></td>
        <td><%= legacy_ulsterbank.trans_value %></td>
        <td><%= legacy_ulsterbank.account_name %></td>
        <td><%= legacy_ulsterbank.account_number %></td>
        <td><%= legacy_ulsterbank.account_name %></td>
        <td><%= link_to 'Show', legacy_ulsterbank %></td>
        <td><%= link_to 'Edit', edit_legacy_ulsterbank_path(legacy_ulsterbank) %></td>
        <td><%= link_to 'Destroy', legacy_ulsterbank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

